{
    "name": "filay-portfolio",
    "version": 2,
    "builds": [
        {
            "src": "**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ],
    "github": {
        "enabled": true,
        "silent": false,
        "autoAlias": true
    }
} 